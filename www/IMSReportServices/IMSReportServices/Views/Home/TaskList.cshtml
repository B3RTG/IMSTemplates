<div class="row">
    <div class="col-md-12">
        <!--<h2>Task List</h2>-->
        <p>Executed/queued job List</p>
    </div>
    <div class="col-md-12">
        <table id="tblTaskList" class="table table-striped">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Job Code</th>
                    <th>Tittle</th>
                    <th>Create Date</th>
                    <th>End Date</th>
                    <th>Status</th>
                    <th>Get Result</th>
                </tr>
            </thead>
        </table>

    </div>
</div>

<div class="col-md-12 alert alert-warning alert-dismissible" role="alert" id="TaskMessage" style="display:none">
    <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <p></p>
</div>

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@Scripts.Render("~/bundles/DataTable")
@Scripts.Render("~/bundles/TaskList")


<script>
    $(document).ready(function () {
        var oTaskModel = new TaskViewControler("tblTaskList");
        oTaskModel.ListAllTask();
    });
</script>